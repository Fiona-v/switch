#include<stdio.h>
void main()
{
int choice=0;
printf("Enter the choice: /n1.Pizza /n2. Burger /n3.Pasta /n4.French fries /n5.Sandwitch");
scanf("%d",choice);
switch(choice)
{
 case 1: printf(" Food- Pizza /nPrice-239");
         break;
 case 2: printf(" Food- Burger /nPrice-129");
         break;
 case 3: printf(" Food- Pasta /nPrice-179");
         break;
 case 4: printf(" Food- French fries  /nPrice-99");
         break;
 case 5: printf(" Food- Sandwitch /nPrice-149");
        break;
 case 6: printf(" INVALID");
}
return (0);
}
